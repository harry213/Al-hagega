     <!--author-->\n                  <span class=\"d-none d-sm-inline arabic-kufi\">\n                    بواسطة <router-link class=\"fw-bold\" to=\"/About\">الحقيقة</router-link>\n                  </span>\n                  <!--date-->\n                  \n                  <time class=\"ms-0 ms-sm-2 ms-md-3\" datetime=\"2019-10-22\">\n                    <svg xmlns=\"http://www.w3.org/2000/svg\" width=\"14\" height=\"14\" fill=\"currentColor\" class=\"bi bi-calendar-check me-1\" viewBox=\"0 0 16 16\">\n                      <path d=\"M10.854 7.146a.5.5 0 0 1 0 .708l-3 3a.5.5 0 0 1-.708 0l-1.5-1.5a.5.5 0 1 1 .708-.708L7.5 9.793l2.646-2.647a.5.5 0 0 1 .708 0z\"/>\n                      <path d=\"M3.5 0a.5.5 0 0 1 .5.5V1h8V.5a.5.5 0 0 1 1 0V1h1a2 2 0 0 1 2 2v11a2 2 0 0 1-2 2H2a2 2 0 0 1-2-2V3a2 2 0 0 1 2-2h1V.5a.5.5 0 0 1 .5-.5zM1 4v10a1 1 0 0 0 1 1h12a1 1 0 0 0 1-1V4H1z\"/>\n                    </svg> {{ news.PostingDate }}\n                  </time>\n                </div>\n                <div class=\"social-share mb-3\">\n                  <!-- share facebook -->\n                  <a class=\"btn btn-social btn-facebook text-white btn-sm blank-windows\" href=\"https://www.facebook.com/sharer.php?u=https://www.hagega.com/#/readShow/53/\" title=\"Share to facebook\">\n                    <svg xmlns=\"http://www.w3.org/2000/svg\" fill=\"currentColor\" class=\"bi bi-facebook me-1 Onerem\" viewBox=\"0 0 16 16\">\n                      <path d=\"M16 8.049c0-4.446-3.582-8.05-8-8.05C3.58 0-.002 3.603-.002 8.05c0 4.017 2.926 7.347 6.75 7.951v-5.625h-2.03V8.05H6.75V6.275c0-2.017 1.195-3.131 3.022-3.131.876 0 1.791.157 1.791.157v1.98h-1.009c-.993 0-1.303.621-1.303 1.258v1.51h2.218l-.354 2.326H9.25V16c3.824-.604 6.75-3.934 6.75-7.951z\"/>\n                    </svg><span class=\"d-none d-sm-inline\">Facebook</span>\n                  </a>\n                  &nbsp;\n                  <!-- share twitter -->\n                  <a class=\"btn btn-social btn-twitter text-white btn-sm blank-windows\" href=\"https://www.twitter.com/share?url=https://www.bootstrap.news\" title=\"Share to twitter\">\n                    <svg xmlns=\"http://www.w3.org/2000/svg\"  fill=\"currentColor\" class=\"bi bi-twitter me-1 Onerem\" viewBox=\"0 0 16 16\">\n                      <path d=\"M5.026 15c6.038 0 9.341-5.003 9.341-9.334 0-.14 0-.282-.006-.422A6.685 6.685 0 0 0 16 3.542a6.658 6.658 0 0 1-1.889.518 3.301 3.301 0 0 0 1.447-1.817 6.533 6.533 0 0 1-2.087.793A3.286 3.286 0 0 0 7.875 6.03a9.325 9.325 0 0 1-6.767-3.429 3.289 3.289 0 0 0 1.018 4.382A3.323 3.323 0 0 1 .64 6.575v.045a3.288 3.288 0 0 0 2.632 3.218 3.203 3.203 0 0 1-.865.115 3.23 3.23 0 0 1-.614-.057 3.283 3.283 0 0 0 3.067 2.277A6.588 6.588 0 0 1 .78 13.58a6.32 6.32 0 0 1-.78-.045A9.344 9.344 0 0 0 5.026 15z\"/>\n                    </svg><span class=\"d-none d-sm-inline text-white\">Twitter</span>\n                  </a>\n                 &nbsp;\n                  <!-- share via email -->\n                  <a class=\"btn btn-social btn-envelope text-white btn-sm\" href=\"mailto:?subject=Your&nbsp;post&nbsp;title&amp;body=Read&nbsp;complete&nbsp;article&nbsp;in&nbsp;here&nbsp;https://www.bootstrap.news\" title=\"Share by Email\">\n                    <svg xmlns=\"http://www.w3.org/2000/svg\"  class=\"me-1 Onerem\" fill=\"currentColor\" viewBox=\"0 0 512 512\"><rect x=\"48\" y=\"96\" width=\"416\" height=\"320\" rx=\"40\" ry=\"40\" style=\"fill:none;stroke:currentColor;stroke-linecap:round;stroke-linejoin:round;stroke-width:32px\"/><polyline points=\"112 160 256 272 400 160\" style=\"fill:none;stroke:currentColor;stroke-linecap:round;stroke-linejoin:round;stroke-width:32px\"/></svg><span class=\"d-none d-sm-inline\">Email</span>\n                  </a>\n                </div>\n              </div>\n              \n            </div>\n            <!-- post content -->\n            <div class=\"post-content\"  v-for=\"(news, index) in feedNews\"\n              v-bind:key=\"index\">\n              <!-- images -->\n              <figure class=\"image-wrapper ratio_single\">\n                <img class=\"img-fluid \" :src=\"news.PostImage\" alt=\"Image description\">\n                {{ new Date() }}\n                <figcaption></figcaption>\n              </figure>\n              <p class=\"arabic-kufi about text-justify\" v-html=\"news.PostDetails\"></p>\n              <!-- images -->\n             \n            </div>\n\n            <!-- footer post -->\n            <div class=\"entry-footer\">\n              <!-- tags -->\n              <div class=\"tags-area\">\n                <ul class=\"tags-links tagcloud ps-0 arabic-kufi\">\n                  <li><span class=\"fw-bold me-2\">Tags</span></li>\n                  <li><router-link to=\"/\">الرئيسية</router-link></li>\n                  <li><router-link to=\"Political\">سياسية</router-link></li>\n                  <li><router-link to=\"Economie\">اقتصادية</router-link></li>\n                  <li><router-link to=\"Article\">مقالات</router-link></li>\n                  <li><router-link to=\"Sport\">رياضة</router-link></li>\n                </ul>\n              </div>\n              <div class=\"gap-0\"></div>\n            </div>\n\n       \n\n            <!--Related post title-->\n            <div class=\"block-area\">\n              <div class=\"block-title-6\">\n                <h4 class=\"h5 border-primary border-2\">\n                  <span class=\"bg-primary text-white arabic-kufi\">مزيد من الاخبار </span>\n                </h4>\n              </div>\n\n               <!-- block content -->\n              <div class=\"row\">\n                <!--post list-->\n                <article class=\"col-sm-6 col-md-12 col-lg-6\" v-for=\"publish in lastnews2\" :key=\"publish.id\">\n                  <!--thumbnail-->\n                    <div class=\"ratio_360-202 image-wrapper\">\n                      <a @click=\"getFeed(publish)\">\n                        <img class=\"img-fluid pointer\" :src=\"publish.PostImage\" alt=\"Image description\">\n                      </a>\n                    </div>\n                    <div class=\"card-body\">\n                      <!-- title -->\n                      <h2 class=\"card-title h1-sm h3-lg arabic-kufi pointer\">\n                        <a @click=\"getFeed(publish)\">{{ publish.PostTitle.substring(0,70) }}</a>\n                      </h2>\n                      <!-- author, date and comments -->\n                      <div class=\"card-text mb-2 text-muted small\">\n                        <span class=\"d-none d-sm-inline me-1 arabic-kufi\">\n                          <a class=\"fw-bold\" href=\"#\">الحقيقة</a>\n                        </span>\n                        <time datetime=\"2019-10-22\">{{ publish.PostingDate }}</time>\n                      </div>\n                      <!--description-->\n                    </div>\n                </article>\n                <div class=\"gap-05\"></div>\n              </div>\n              <!-- end block content -->\n              </div>\n     \n          </div>\n          <!-- end content -->\n          <!-- start right column -->\n        \n      \n        <LastNews />\n        \n\n\n        </div>\n      </div>\n    </main>\n    </template>\n      <template #fallback>\n        <Loading />\n      </template>\n    </Suspense>\n    <Footer />\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport { Suspense } from \"vue\";\nimport Loading from \"@/components/Include/Loading.vue\";\nimport Header from \"@/components/Include/Header.vue\";\nimport Footer from \"@/components/Include/Footer.vue\";\nimport LastNews from \"@/components/Pages/LastNews.vue\";\nexport default {\n    name: 'ReadShow',\n    props: [\"id\"],\n    data() {\n    return {\n      publishId: \"\",\n      feedNews: \"\",\n      // lastnews: \"\",\n      // lastnews1: \"\",\n      lastnews2: \"\",\n      url: \"https://github.com/mbj36\"\n    };\n  },\n   components: {\n    Header,\n    Footer,\n    Loading,\n    Suspense,\n    LastNews\n  },\n  // created() {\n  //   this.Read_Content_id = this.$cookie.getCookie(\"Read_Content_id\");\n  // },\n  // mounted() {\n  //     axios\n  //     .get(\"readPost.php?readPost&id=\"+this.Read_Content_id)\n  //     .then((response) => (this.feedNews = response.data.readPost));\n  // },\n  // async setup() {\n  //   // const feedNews = await axios.get(\"readPost.php?readPost&id=\"+this.$cookie.getCookie(\"Read_Content_id\"))\n  //   //       .then((response) => { return response.data.readPost; });\n  //   const lastnews = await axios.get(\"getPost.php?getPost&LIMIT=1&OFFSET=0\")\n  //         .then((response) => { return response.data.getPost; });\n  //   const lastnews1 = await axios.get(\"getPost.php?getPost&LIMIT=3&OFFSET=1\")\n  //         .then((response) => { return response.data.getPost; });\n  //         const lastnews2 = await axios.get(\"getPost.php?getPost&LIMIT=2&OFFSET=5\")\n  //         .then((response) => { return response.data.getPost; });\n  //   return {\n  //     // feedNews,\n  //     lastnews,\n  //     lastnews1,\n  //     lastnews2\n  //   }\n  // },\n\n methods: {\n    initData: function (id) {\n      let queryString = \"\";\n      queryString = \"readPost.php?readPost&id=\" + id;\n      axios.get(queryString).then((response) => {\n        this.feedNews = response.data.readPost;\n      });\n      // axios\n      //   .get(\n      //     \"getPost.php?getPost&LIMIT=1&OFFSET=0\"\n      //   )\n      //   .then((response) => {\n      //     this.lastnews = response.data.getPost;\n      //   });\n      //   axios\n      //   .get(\n      //     \"getPost.php?getPost&LIMIT=3&OFFSET=1\"\n      //   )\n      //   .then((response) => {\n      //     this.lastnews1 = response.data.getPost;\n      //   });\n        axios\n        .get(\n          \"getPost.php?getPost&LIMIT=2&OFFSET=5\"\n        )\n        .then((response) => {\n          this.lastnews2 = response.data.getPost;\n        });\n      \n    },\n    getFeed: function (publish) {\n      this.$router.push({ name: \"ReadShow\", params: { id: publish.id } });\n      this.$router.go()\n    },\n  },\n  created: function () {\n    this.publishId = this.$route.params.id;\n    this.initData(this.publishId);\n  },\n\n}\n</script>\n\n<style scoped>\n.arabic-kufi {\n  font-family: 'Droid Arabic Kufi', serif;\n}\n.pointer {\n  cursor: pointer;\n}\n.Onerem {\n  width: 1rem;\n  height: 1rem\n}\n</style>"],"sourceRoot":""}]}