 0 1-2-2V3a2 2 0 0 1 2-2h1V.5a.5.5 0 0 1 .5-.5zM1 4v10a1 1 0 0 0 1 1h12a1 1 0 0 0 1-1V4H1z\"\n                          />\n                        </svg>\n                        {{ publish.PostingDate }}\n                      </time>\n                    </div>\n\n                    <div class=\"social-share mb-3\">\n                      <!-- share facebook -->\n                      <a\n                        class=\"\n                          btn btn-social btn-facebook\n                          text-white\n                          btn-sm\n                          blank-windows\n                        \"\n                        href=\"https://www.facebook.com/sharer.php?u=https://www.hagega.com\"\n                        target=\"_blank\"\n                        title=\"Share to facebook\"\n                      >\n                        <svg\n                          xmlns=\"http://www.w3.org/2000/svg\"\n                          fill=\"currentColor\"\n                          class=\"bi bi-facebook me-1 Onerem\"\n                          viewBox=\"0 0 16 16\"\n                        >\n                          <path\n                            d=\"M16 8.049c0-4.446-3.582-8.05-8-8.05C3.58 0-.002 3.603-.002 8.05c0 4.017 2.926 7.347 6.75 7.951v-5.625h-2.03V8.05H6.75V6.275c0-2.017 1.195-3.131 3.022-3.131.876 0 1.791.157 1.791.157v1.98h-1.009c-.993 0-1.303.621-1.303 1.258v1.51h2.218l-.354 2.326H9.25V16c3.824-.604 6.75-3.934 6.75-7.951z\"\n                          /></svg\n                        ><span class=\"d-none d-sm-inline\">Facebook</span>\n                      </a>\n                      &nbsp;\n                      <!-- share twitter -->\n                      <a\n                        class=\"\n                          btn btn-social btn-twitter\n                          text-white\n                          btn-sm\n                          blank-windows\n                        \"\n                        href=\"https://www.twitter.com/share?url=https://www.hagega.com\"\n                        title=\"Share to twitter\"\n                      >\n                        <svg\n                          xmlns=\"http://www.w3.org/2000/svg\"\n                          fill=\"currentColor\"\n                          class=\"bi bi-twitter me-1 Onerem\"\n                          viewBox=\"0 0 16 16\"\n                        >\n                          <path\n                            d=\"M5.026 15c6.038 0 9.341-5.003 9.341-9.334 0-.14 0-.282-.006-.422A6.685 6.685 0 0 0 16 3.542a6.658 6.658 0 0 1-1.889.518 3.301 3.301 0 0 0 1.447-1.817 6.533 6.533 0 0 1-2.087.793A3.286 3.286 0 0 0 7.875 6.03a9.325 9.325 0 0 1-6.767-3.429 3.289 3.289 0 0 0 1.018 4.382A3.323 3.323 0 0 1 .64 6.575v.045a3.288 3.288 0 0 0 2.632 3.218 3.203 3.203 0 0 1-.865.115 3.23 3.23 0 0 1-.614-.057 3.283 3.283 0 0 0 3.067 2.277A6.588 6.588 0 0 1 .78 13.58a6.32 6.32 0 0 1-.78-.045A9.344 9.344 0 0 0 5.026 15z\"\n                          /></svg\n                        ><span class=\"d-none d-sm-inline text-white\"\n                          >Twitter</span\n                        >\n                      </a>\n                      &nbsp;\n                      <!-- share via email -->\n                      <a\n                        class=\"btn btn-social btn-envelope text-white btn-sm\"\n                        href=\"mailto:?subject=Your&nbsp;post&nbsp;title&amp;body=Read&nbsp;complete&nbsp;article&nbsp;in&nbsp;here&nbsp;https://www.hagega.com\"\n                        title=\"Share by Email\"\n                      >\n                        <svg\n                          xmlns=\"http://www.w3.org/2000/svg\"\n                          class=\"me-1 Onerem\"\n                          fill=\"currentColor\"\n                          viewBox=\"0 0 512 512\"\n                        >\n                          <rect\n                            x=\"48\"\n                            y=\"96\"\n                            width=\"416\"\n                            height=\"320\"\n                            rx=\"40\"\n                            ry=\"40\"\n                            style=\"\n                              fill: none;\n                              stroke: currentColor;\n                              stroke-linecap: round;\n                              stroke-linejoin: round;\n                              stroke-width: 32px;\n                            \"\n                          />\n                          <polyline\n                            points=\"112 160 256 272 400 160\"\n                            style=\"\n                              fill: none;\n                              stroke: currentColor;\n                              stroke-linecap: round;\n                              stroke-linejoin: round;\n                              stroke-width: 32px;\n                            \"\n                          /></svg\n                        ><span class=\"d-none d-sm-inline\">Email</span>\n                      </a>\n                    </div>\n                  </div>\n                </div>\n                <!-- post content -->\n                <div\n                  class=\"post-content\"\n                  v-for=\"(publish, index) in feedNews\"\n                  v-bind:key=\"index\"\n                >\n                  <!-- images -->\n                  <figure class=\"image-wrapper ratio_single\">\n                    <img\n                      class=\"img-fluid\"\n                      :src=\"publish.PostImage\"\n                      alt=\"Image description\"\n                    />\n                    {{ new Date() }}\n                    <figcaption></figcaption>\n                  </figure>\n                  <p\n                    class=\"arabic-kufi about text-justify\"\n                    v-html=\"publish.PostDetails\"\n                  ></p>\n                  <!-- images -->\n                </div>\n\n                <!-- footer post -->\n                <div class=\"entry-footer\">\n                  <!-- tags -->\n                  <div class=\"tags-area\">\n                    <ul class=\"tags-links tagcloud ps-0 arabic-kufi\">\n                      <li><span class=\"fw-bold me-2\">Tags</span></li>\n                      <li><router-link to=\"/\">الرئيسية</router-link></li>\n                      <li><router-link to=\"/Political\">سياسية</router-link></li>\n                      <li>\n                        <router-link to=\"/Economie\">اقتصادية</router-link>\n                      </li>\n                      <li><router-link to=\"/Article\">مقالات</router-link></li>\n                      <li><router-link to=\"/Sport\">رياضة</router-link></li>\n                    </ul>\n                  </div>\n                  <div class=\"gap-0\"></div>\n                </div>\n\n                <!--Related post title-->\n                <div class=\"block-area\">\n                  <div class=\"block-title-6\">\n                    <h4 class=\"h5 border-primary border-2\">\n                      <span class=\"bg-primary text-white arabic-kufi\"\n                        >مزيد من الاخبار\n                      </span>\n                    </h4>\n                  </div>\n\n                  <!-- block content -->\n                  <div class=\"row\">\n                    <!--post list-->\n                    <article\n                      class=\"col-sm-6 col-md-12 col-lg-6\"\n                      v-for=\"publish in lastnews2\"\n                      :key=\"publish.id\"\n                    >\n                      <!--thumbnail-->\n                      <div class=\"ratio_360-202 image-wrapper\">\n                        <a @click=\"getFeed(publish)\">\n                          <img\n                            class=\"img-fluid pointer\"\n                            :src=\"publish.PostImage\"\n                            alt=\"Image description\"\n                          />\n                        </a>\n                      </div>\n                      <div class=\"card-body\">\n                        <!-- title -->\n                        <h2 class=\"card-title h1-sm h3-lg arabic-kufi pointer\">\n                          <a @click=\"getFeed(publish)\">{{\n                            publish.PostTitle.substring(0, 70)\n                          }}</a>\n                        </h2>\n                        <!-- author, date and comments -->\n                        <div class=\"card-text mb-2 text-muted small\">\n                          <span class=\"d-none d-sm-inline me-1 arabic-kufi\">\n                            <a class=\"fw-bold\" href=\"#\">الحقيقة</a>\n                          </span>\n                          <time datetime=\"2019-10-22\">{{\n                            publish.PostingDate\n                          }}</time>\n                        </div>\n                        <!--description-->\n                      </div>\n                    </article>\n                    <div class=\"gap-05\"></div>\n                  </div>\n                  <!-- end block content -->\n                </div>\n              </div>\n              <!-- end content -->\n              <!-- start right column -->\n              <LastNews />\n            </div>\n          </div>\n        </main>\n      </template>\n      <template #fallback>\n        <Loading />\n      </template>\n    </Suspense>\n    <Footer />\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport { Suspense } from \"vue\";\nimport Loading from \"@/components/Include/Loading.vue\";\nimport Header from \"@/components/Include/Header.vue\";\nimport Footer from \"@/components/Include/Footer.vue\";\nimport LastNews from \"@/components/Pages/LastNews.vue\";\nimport { watch, watchEffect } from \"vue\";\nimport { ref } from \"vue\";\nimport { useMeta } from \"vue-meta\";\nimport { useRouter, useRoute } from \"vue-router\";\nexport default {\n  name: \"Reads\",\n  props: {\n    id: String,\n  },\n  // metaInfo () {\n  //   return {\n  //     title: window.localStorage.getItem('title'),\n  //     meta: [\n  //       { name: 'description', content: 'الحقيقة ديلي برس', vmid: 'test' },\n  //       { name: 'og:description', content: window.localStorage.getItem('PostDetails') },\n  //       { name: 'og:image', content: window.localStorage.getItem('PostImage') }\n  //     ],\n  //   }\n  // },\n   setup(props) {\n     const router = useRouter();\n     const route = useRoute();\n     watch(() => route.params.id,(previous, current) => {\n        // console.log(`${previous} and ${current}`);\n        window.scrollTo(0, 0);\n      },\n      { deep: true }\n    );\n\n    const feedNews = ref([]);\n    const lastnews2 = ref([]);\n\n    axios.get(\"readPost.php?readPost&id=\" + props.id).then((response) => {\n      feedNews.value = response.data.readPost;\n    });\n    axios.get(\"getPost.php?getPost&LIMIT=2&OFFSET=5\").then((response) => {\n      lastnews2.value = response.data.getPost;\n    });\n    const getFeed = (publish) => {\n      router.push({ name: \"Reads\", params: { id: publish.id } });\n      const PostTitle = publish.PostTitle\n      console.log(\"PostTitle ====== \" + PostTitle);\n    };\n    \n    const { meta } = useMeta({\n      title: \"السودان يشرع في الاعداد لقيام بورصة للسلع الزراعية\",\n    });\n\n    watchEffect(() => {\n      const counterValue = lastnews2.value;\n      meta.description = `Counted ${counterValue} times`;\n      // const counterValue = publish.PostTitle\n      // meta.description = `Counted  times`;\n    });\n\n    return {\n      feedNews,\n      lastnews2,\n      getFeed,\n    };\n\n    // useMeta({\n    //   title: 'السودان يشرع في الاعداد لقيام بورصة للسلع الزراعية',\n    //   htmlAttrs: {\n    //     lang: 'ar',\n    //     amp: true\n    //   }\n    // })\n  },\n\n  // head() {\n  //   return {\n  //     title: this.publish.PostTitle,\n  //     meta: [\n  //       {\n  //         hid: \"description\",\n  //         name: \"description\",\n  //         content: this.publish.PostTitle,\n  //       },\n  //       {\n  //         hid: \"og:title\",\n  //         name: \"og:title\",\n  //         content: this.publish.PostTitle,\n  //       },\n  //       {\n  //         hid: \"og:image\",\n  //         property: \"og:image\",\n  //         content: `/${this.publish.PostImage}`,\n  //       },\n  //       {\n  //         hid: \"og:description\",\n  //         property: \"og:description\",\n  //         content: this.publish.PostTitle,\n  //       },\n  //       {\n  //         hid: \"og:url\",\n  //         property: \"og:url\",\n  //         content: `https://hagega.com/reads/${this.publish.id}`,\n  //       },\n  //     ],\n  //   };\n  // },\n\n  components: {\n    Header,\n    Footer,\n    Loading,\n    Suspense,\n    LastNews,\n  },\n\n  methods: {\n    initData: function () {\n      // let queryString = \"\";\n      // queryString = \"readPost.php?readPost&id=\" + id;\n      // axios.get(queryString).then((response) => {\n      //   this.feedNews = response.data.readPost;\n      // });\n      // axios.get(\"getPost.php?getPost&LIMIT=2&OFFSET=5\").then((response) => {\n      //   this.lastnews2 = response.data.getPost;\n      // });\n      // document.addEventListener(\n      //   \"keydown\",\n      //   function () {\n      //     if (event.keyCode == 123) {\n      //       // alert(\"This function has been disabled to prevent you from stealing my code!\");\n      //       return false;\n      //     } else if (event.ctrlKey && event.shiftKey && event.keyCode == 73) {\n      //       // alert(\"This function has been disabled to prevent you from stealing my code!\");\n      //       return false;\n      //     } else if (event.ctrlKey && event.keyCode == 85) {\n      //       // alert(\"This function has been disabled to prevent you from stealing my code!\");\n      //       return false;\n      //     }\n      //   },\n      //   false\n      // );\n      // if (document.addEventListener) {\n      //   document.addEventListener(\n      //     \"contextmenu\",\n      //     function (e) {\n      //       // alert(\"This function has been disabled to prevent you from stealing my code!\");\n      //       e.preventDefault();\n      //     },\n      //     false\n      //   );\n      // } else {\n      //   document.attachEvent(\"oncontextmenu\", function () {\n      //     // alert(\"This function has been disabled to prevent you from stealing my code!\");\n      //     window.event.returnValue = false;\n      //   });\n      // }\n    },\n    // getFeed: function (publish) {\n    //   this.$router.push({ name: \"Reads\", params: { id: publish.id } });\n    // },\n  },\n  // created() {\n  //  this.publishId = this.$route.query.id;\n  // },\n  // created: function () {\n  //   this.publishId = this.$route.params.id;\n  //   this.initData(this.publishId);\n  //           console.log(\"publishId =========\" + this.publishId)\n\n  // },\n};\n</script>\n\n<style scoped>\n.arabic-kufi {\n  font-family: \"Droid Arabic Kufi\", serif;\n}\n.pointer {\n  cursor: pointer;\n}\n.Onerem {\n  width: 1rem;\n  height: 1rem;\n}\n</style>"],"sourceRoot":""}]}